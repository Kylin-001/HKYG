        "search/index",
        "map/index",
        "dorm/index",
        "library/index",
        "library/seat-reservation",
        "library/book-search",
        "library/book-detail"
      ],
      "independent": false
    },
    {
      "root": "pages/courier",
      "pages": [
        "index",
        "publish/index",
        "detail/index",
        "rate/index",
        "locker/index"
      ],
      "independent": false
    },
    {
      "root": "pages/product",
      "pages": [
        "list/index",
        "detail/index"
      ],
      "independent": false
