        "list/index"
      ],
      "independent": false
    },
    {
      "root": "pages/admin",
      "pages": [
        "dashboard"
      ],
      "independent": false
    },
    {
      "root": "pages/forum",
      "pages": [
        "index",
        "detail/index",
        "publish/index",
        "search/index"
      ],
      "independent": false
    },
    {
      "root": "pages/activity",
      "pages": [
        "list/index",
        "detail/index"
