{
  "name": "heikeji-campus-service-frontend",
  "version": "1.0.0",
  "description": "黑科易购校园服务平台前端项目",
  "author": "heikeji",
  "private": true,
  "scripts": {
    "dev": "vite",
    "serve": "vite preview",
    "start": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:preview": "vite build --mode preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.json --composite false",
    "test:unit": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write \"src/**/*.{vue,js,ts,jsx,tsx,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{vue,js,ts,jsx,tsx,css,scss}\"",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/runtime": "^7.25.0",
    "axios": "^1.6.0",
    "cssnano": "^6.0.0",
    "echarts": "^5.4.0",
    "element-plus": "^2.4.4",
    "express": "^4.18.2",
    "js-cookie": "^3.0.5",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.5.1",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.25.0",
    "@babel/preset-env": "^7.25.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.18.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-pxtorem": "^6.1.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.69.0",
    "terser": "^5.44.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^3.0.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^4.0.14",
    "vue-tsc": "^1.8.0"
  },
  "engines": {
    "node": ">= 18.0.0",
    "npm": ">= 9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
